#include <stdio.h>

#define MAXNOME 11
/*Síntese:
Objetivo: registrar partidas de par ou ímpar e indicar vencedor.
Entradas: nro de partidas, nome dos jogadores, nros jogados.
Saídas:  identificador da partida, nome do vencedor, \n.*/

int main(){
	int nroPartidas, aux=0, soma, identPartida=1;
	char nome1[MAXNOME];
	char nome2[MAXNOME];
	scanf(" %d", &nroPartidas);
	if(nroPartidas<0||nroPartidas>=1000){
		exit(1);
	}
	int mao1[nroPartidas];
	int mao2[nroPartidas];
	
	scanf(" %s", nome1);
	scanf(" %s", nome2);
	if(strlen(nome1)<=1 || strlen(nome2)<=1)
		exit(1);
	for(aux=0; aux<nroPartidas; aux++) {
		scanf("%d %d", &mao1[aux], &mao2[aux]);
		if(mao1[aux]<0||mao1[aux]>5||mao2[aux]<0||mao2[aux]>5)
			break;
	}
	printf("Teste %d\n", identPartida);
	for(aux=0; aux<nroPartidas; aux++){
		soma = mao1[aux] + mao2[aux];
		if(soma%2==1)
			puts(nome2);
		else
			puts(nome1);	
	}		
	identPartida++;
	
	
	
	return 0;
}
