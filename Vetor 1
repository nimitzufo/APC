#include <stdio.h>

/*Síntese:
Objetivo: encontrar o segundo e terceiro menores valores de um vetor.
Entradas: tamanho do vetor, valores do vetor.
Saídas:  primeira linha, segundo menor valor, segunda linha, terceiro menor.*/

int main(){
	int tamV=0, aux=0, m1, m2, m3;
	
	scanf("%d", &tamV);
	int V[tamV];
	for(aux=0;aux<tamV;aux++)
		scanf("%d", &V[aux]);
	aux=0;
		
	//Verifica menor valor
	for(aux=0;aux<(tamV-1);aux++){
		if(V[aux]<V[aux+1]){
			m1=V[aux];
		}	
	}
	aux=0;
	//verifica o segundo menor valor
	for(aux=0;aux<(tamV-1);aux++){
		if(V[aux]<V[aux+1] && V[aux]!=m1)
			m2=V[aux];
	}
	aux=0;
	//verifica o terceiro menor valor
	for(aux=0;aux<(tamV-1);aux++){
		if(V[aux]<=V[0] && V[aux]!=m1 && V[aux]!=m2){
				m3=V[aux];	
		}
	}
	
	printf("%d\n%d\n%d\n",m1, m2, m3);
	
	return 0;
}
